{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "route": "{*restOfPath}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}